<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/bodydata"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="100dp"
        android:onClick="bodydata"
        android:text="Body Data"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/questionnarie"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="56dp"
        android:onClick="questionnaire"
        android:text="Questionnarie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bodydata" />

    <Button
        android:id="@+id/model"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="56dp"
        android:onClick="model"
        android:text="Model"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questionnarie" />

    <Button
        android:id="@+id/button7"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="56dp"
        android:onClick="lung"
        android:text="Lung recovery"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/model"
        app:layout_constraintVertical_bias="0.029" />

</androidx.constraintlayout.widget.ConstraintLayout>